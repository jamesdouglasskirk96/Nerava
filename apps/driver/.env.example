# Driver App Environment Variables
# Copy to .env and fill in real values

# API Configuration
VITE_API_BASE_URL=http://localhost:8001

# Google Places API
VITE_GOOGLE_PLACES_API_KEY=your-google-places-key

# Environment
VITE_ENV=development

# Development/Demo Mode (set to false in production)
VITE_DEMO_MODE=false
VITE_MOCK_MODE=false

# iOS Native Bridge (set to true when running inside iOS WKWebView shell)
VITE_NATIVE_BRIDGE_ENABLED=false
